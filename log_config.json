{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    },
    "json": {
      "format": "{\"timestamp\":\"%(asctime)s\",\"name\":\"%(name)s\",\"level\":\"%(levelname)s\",\"message\":\"%(message)s\"}",
      "datefmt": "%Y-%m-%d %H:%M:%S"
    }
  },
  "handlers": {
    "default": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "json": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "": {
      "level": "INFO",
      "handlers": ["json"]
    },
    "uvicorn": {
      "level": "INFO",
      "handlers": ["default"],
      "propagate": false
    },
    "uvicorn.access": {
      "level": "INFO",
      "handlers": ["default"],
      "propagate": false
    },
    "uvicorn.error": {
      "level": "INFO",
      "handlers": ["default"],
      "propagate": false
    }
  }
}

